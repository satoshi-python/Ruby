#! ruby -Ku
# coding: utf-8

def check(str)
  print(str, "is ^abc ")
  if /^abc/ =~ str then
    print("match.\n")
  else
    print("not match.\n")
  end
end

check("abcdef")
check("defabc")
check("abc짜ndef")
check("def짜nabc")

print("\n")

def check2(str)
  print(str, " is abc$ ")
  if /abc$/ =~ str then
    print(" match.\n ")
  else
    print(" not match.\n")
  end
end

check2("abcdef")
check2("defabc")
check2("abc짜ndef")
check2("def짜nabc")
